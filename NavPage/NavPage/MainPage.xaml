<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="NavPage.MainPage">

    <ScrollView>
        <VerticalStackLayout
            Padding="30,0"
            Spacing="25">

            <Entry x:Name="nombre" Placeholder="Nombre"></Entry>
            <Entry x:Name="apellidos" Placeholder="Apellidos"></Entry>

            <Button
                x:Name="Tabbed"
                Text="Pagina Tabbed" 
                Clicked="IrTabbed"
                HorizontalOptions="Fill" />
            <Button
                x:Name="pagina4"
                Text="Pagina 4" 
                Clicked="IrPagina4"
                HorizontalOptions="Fill" />
            <Button
                x:Name="pagina5"
                Text="Pagina 5" 
                Clicked="IrPagina5"
                HorizontalOptions="Fill" />
        </VerticalStackLayout>
    </ScrollView>

</ContentPage>
